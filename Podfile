  use_frameworks!

  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'GoogleSignIn'
  pod 'FacebookCore'#(0.5.0)
  pod 'FacebookLogin'#(0.5.0)
  pod 'FBSDKCoreKit','~>4.38.0'
  pod 'FBSDKLoginKit','~>4.38.0'

  target 'ShowTimeAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
